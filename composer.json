{
    "name": "rampall/laravel-console-task",
    "description": "Laravel Console Task is a output method for your Laravel/Laravel Zero commands.",
    "keywords": [
        "console",
        "command-line",
        "php",
        "cli",
        "laravel-zero",
        "laravel",
        "artisan",
        "symfony"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rampall/laravel-console-task/issues",
        "source": "https://github.com/rampall/laravel-console-task"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "rameshpallikara@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.0"
    },
    "autoload": {
        "psr-4": {
            "RamPall\\LaravelConsoleTask\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RamPall\\LaravelConsoleTask\\LaravelConsoleTaskServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}